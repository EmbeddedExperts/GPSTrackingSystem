using DevExpress.XtraMap;
using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace GpsProject
{
    public class Node
    {
        public GeoPoint Value { get; set; }

        public Node After { get; set; }

        public Node Before { get; set; }

        public Node(GeoPoint Value)
        {
            this.Value = Value;
            this.Before = null;
            this.After = null;
        }
    }
}
